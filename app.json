{
  "expo": {
    "name": "Buscomida",
    "slug": "Buscomida",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00a680"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pixellab.buscomida",
      "permissions": ["CAMERA_ROLL"]
    },
    "packagerOpts": {
      "nonPersistent": "--nonPersistent"
}
  }
}
